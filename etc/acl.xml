<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="SyncitGroup_SgForm::sgformconfig" title="Syncit Group Custom Contact Form" sortOrder="50"></resource>
                        </resource>
                    </resource>
                </resource>
                <resource id="Magento_Catalog::catalog">
					<resource id="SyncitGroup_SgForm::sgform_group" title="Syncit Group" translate="title" sortOrder="10">
						<resource id="SyncitGroup_SgForm::sgform" title="Contact Form" translate="title" sortOrder="23">
						</resource>	
					</resource>
				</resource>
			</resource>
        </resources>
    </acl>
</config>